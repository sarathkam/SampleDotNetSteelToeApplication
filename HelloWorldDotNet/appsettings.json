{
  "spring": {
    "application": {
      "name": "dotnet-webapp"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validate_certifates": false
      }
    }
  },
  
    "message": "Message picked from local configuration!!!",
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": false,
      "validate_certificates": false
    },
    "instance": {
      "hostName": "localhost",
      "port": 5000
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Pivotal": "Debug",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*"
}
